}
.eat-type .choosed {
	background-color: #CB9B0C;
	border-radius: 25rpx;
	-webkit-border-radius: 25rpx;
	-moz-border-radius: 25rpx;
	color: #fff;
}


page{
	height:100%;
	padding-top:240rpx;
	position: relative;
}
.goods-area{
	height:100%;
	background-color: #fff;
	border-top:1rpx solid #EAEBF0;
}
.goods-area-bottom {
	padding-bottom: 100rpx;
}
.forbid-click{
