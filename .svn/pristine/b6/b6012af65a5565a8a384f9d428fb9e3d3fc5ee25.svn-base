/* 优惠券样式 */

.ticket-mark{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
  background-color: rgba(0,0,0,0.45);
}
.ticket{
  width: 286px;
  height: 300px;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  background-size: 100%;
  z-index: 101;
  padding: 60rpx;
}
.ticket-img{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
.ticket-title{
  font-size: 34rpx;
  font-weight: bold;
  color: #D83A16;
  text-align: center;
  position: relative;
}
.ticket-content{
  width: 100%;
  /* height: 90rpx; */
  padding: 10rpx;
  /* position: relative; */
  display: flex;
  justify-content: center;
  align-items: center;
  background-image: linear-gradient(to right, #FF2906 , #F45100);
  border-radius: 20rpx;
  /* top: 20rpx; */
  margin-bottom: 10rpx;
}
.ticket-icon{
  font-size: 40rpx;
  font-weight: bold;
  align-self: flex-end;
}
.ticket-money{
  font-size: 40rpx;
	font-weight: bold;
	align-self: flex-end;
}
.ticket-content>view:first-child{display: flex;color: #ffffff;padding: 0 10rpx;width: 33%;justify-content: center;
	align-items: center;}
.ticket-botton{
  background-image: linear-gradient(to right, #FFC746 , #FB763B);
}
.ticket-content>view:last-child{display: flex;width: 65%;flex-direction: column;justify-content: center;background-color: #FFF4BF;padding: 0 20rpx;border-bottom-right-radius: 10rpx;border-top-right-radius: 10rpx;}
.ticket-name{color:#FF553E;font-weight: bold;font-size: 13px;}
.ticket-text{color:#666666;font-size: 13px;}
.ticket-botton{background-image: linear-gradient(to right, #FFC746 , #FB763B);color: #ffffff;font-weight: bold;font-size: 28rpx;position: relative;line-height: 88rpx;text-align: center;top: 70rpx;border-radius: 88rpx;box-shadow: 0px 2px 0px #FCE0B5;}
.ticket-close{position: absolute;left: 50%;bottom: -80rpx;transform: translateX(-50%);width: 61rpx;height: 61rpx;}
.ticket-close image{width: 100%;height: 100%;}
.ticket-contents{height: 120px;overflow: auto;display: flex;flex-direction: column;align-items: center;position:relative;margin-top: 20rpx;}
.ticket-contents>view:first-child{margin-top: 10px;}

